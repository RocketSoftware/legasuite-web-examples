<rule>
  <matches>
    <match>
      <container id="button" template="Button">
        <field id="label" min_occurrence="1" search_direction="horizontal">
          <attributes>
            <editable>true</editable>
            <intensified>true</intensified>
          </attributes>
          <content>
            <regexp pattern="\S+"/>
          </content>
        </field>
      </container>
    </match>
  </matches>
</rule>
